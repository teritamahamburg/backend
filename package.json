{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "serve": "nodemon --watch src --watch ./schema.graphql --ext js,graphql --exec \"webpack --mode development --display none & node dist\"",
    "build": "webpack --mode production",
    "start": "node dist",
    "start:test": "cross-env NODE_ENV=test npm run start",
    "db:migrate": "sequelize db:migrate --config src/sequelize/config/config.js --migrations-path src/sequelize/migrations --seeders-path src/sequelize/seeders --models-path src/sequelize/models"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "apollo-server-koa": "^2.6.9",
    "debug": "^4.1.1",
    "dottie": "^2.0.1",
    "graphql": "^14.4.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-history-api-fallback": "^1.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "sequelize": "^5.10.0",
    "sequelize-cli": "^5.5.0",
    "sqlite3": "^4.0.9",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
